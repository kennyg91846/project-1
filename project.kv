Interface:
    
    TextInput:
        id: textinput
        size_hint: 0.5, 0.1
        pos_hint: {'center_x': 0.5, 'center_y': 0.6}
        multiline: False 
        text: ''

    Label: 
        id: label
        text: 'Nothing Is Here Yet'
        size_hint: 0.5, 0.1
        pos_hint: {'center_x': 0.5, 'center_y': 0.5}
        text: 'Coming Info'

    Button:
        text: 'Press Me'
        size_hint: 0.5, 0.1
        pos_hint: {'center_x': 0.5, 'center_y': 0.4}
        on_press: root.display_information()